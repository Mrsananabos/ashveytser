<?xml version="1.0" encoding="ISO-8859-1"?>
<hibernate-mapping>
    <class name="ru.job4j.carMarket.model.entity.Car" table="car">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <many-to-one name="model"
                     column="model_id"
                     class="ru.job4j.carMarket.model.entity.Model"/>
        <many-to-one name="mark"
                     column="mark_id"
                     class="ru.job4j.carMarket.model.entity.Mark"/>
        <many-to-one name="transmission"
                     column="trans_id"
                     class="ru.job4j.carMarket.model.entity.Transmission"/>
        <many-to-one name="bodyType"
                     column="body_type_id"
                     class="ru.job4j.carMarket.model.entity.BodyType"/>
        <many-to-one name="user"
                     column="user_id"
                     class="ru.job4j.carMarket.model.entity.User"/>
        <property name="yearIssue" column="year_issue"/>
        <property name="price" column="price"/>
    </class>
</hibernate-mapping>
